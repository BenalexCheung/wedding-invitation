@import "/style/black-gold.wxss";
@import "/style/animate.wxss"
@import "/style/animate-extend.wxss"

page{
  /*color: #b69058;*/
  width: 100vw;
  min-height: 100vh;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.container{
  min-width: 100vw;
  min-height: 100vh;
  box-sizing: border-box;
  position: relative;
}
.fixed{
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  overflow: hidden;
}
.bg-father{
  position: relative;
  overflow: hidden;
}
.bg{
  position: absolute;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  overflow: hidden;
}
.overflow{
  overflow: hidden;
}
.mar-b8{
  margin-bottom: 8rpx;
}
.pad20{
  padding: 20rpx;
}
.pad40{
  padding: 40rpx;
}
.font24{
  font-size: 24rpx;
}
.font28{
  font-size: 28rpx;
}
.bold{
  font-weight: bold;
}
.max{
  height: 100%;
  width: 100%;
  position: relative;
  box-sizing: border-box;
}
.center{
  text-align: center;
}
.flex-wrap,
.flex-center-wrap{
  display: flex;
}
.flex-center-wrap {
  -webkit-align-items: center;
  align-items: center;
}
.flex-justify-between {
  -webkit-justify-content: space-between;
  justify-content: space-between;
}
.flex-justify-around {
  -webkit-justify-content: space-around;
  justify-content: space-around;
}
